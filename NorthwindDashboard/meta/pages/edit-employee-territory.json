{
  "access": [
    "Everybody"
  ],
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "data": "${employeeterritory}",
                  "events": {
                    "cancel": [
                      {
                        "type": "back"
                      }
                    ],
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update EmployeeTerritory",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "Northwind.updateEmployeeTerritory",
                        "parameters": [
                          {
                            "name": "EmployeeID",
                            "value": "${parameters.EmployeeID}"
                          },
                          {
                            "name": "TerritoryID",
                            "value": "${parameters.TerritoryID}"
                          },
                          {
                            "name": "EmployeeTerritory",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "name": "Northwind.getEmployees",
                        "condition": "event.property == 'EmployeeID'",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "${event.top}"
                          },
                          {
                            "name": "$skip",
                            "value": "${event.skip}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$count",
                            "value": "true"
                          }
                        ],
                        "then": [
                          {
                            "name": "getEmployeesResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getEmployeesCount",
                            "type": "set",
                            "value": "result['@odata.count']"
                          }
                        ],
                        "type": "invoke"
                      },
                      {
                        "name": "Northwind.getTerritories",
                        "condition": "event.property == 'TerritoryID'",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "${event.top}"
                          },
                          {
                            "name": "$skip",
                            "value": "${event.skip}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$count",
                            "value": "true"
                          }
                        ],
                        "then": [
                          {
                            "name": "getTerritoriesResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getTerritoriesCount",
                            "type": "set",
                            "value": "result['@odata.count']"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "fields": [],
                  "name": "form0",
                  "type": "form"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "Northwind.getEmployeeTerritoryById",
        "parameters": [
          {
            "name": "EmployeeID",
            "value": "${parameters.EmployeeID}"
          },
          {
            "name": "TerritoryID",
            "value": "${parameters.TerritoryID}"
          }
        ],
        "then": [
          {
            "name": "employeeterritory",
            "type": "set",
            "value": "${result}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getEmployees",
        "parameters": [
          {
            "name": "$top",
            "value": "(<any>this.form0.fields.find(f => f.property == 'EmployeeID')).pageSize"
          },
          {
            "name": "$skip",
            "value": "0"
          },
          {
            "name": "$count",
            "value": "true"
          }
        ],
        "then": [
          {
            "name": "getEmployeesResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getEmployeesCount",
            "type": "set",
            "value": "result['@odata.count']"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "Northwind.getTerritories",
        "parameters": [
          {
            "name": "$top",
            "value": "(<any>this.form0.fields.find(f => f.property == 'TerritoryID')).pageSize"
          },
          {
            "name": "$skip",
            "value": "0"
          },
          {
            "name": "$count",
            "value": "true"
          }
        ],
        "then": [
          {
            "name": "getTerritoriesResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getTerritoriesCount",
            "type": "set",
            "value": "result['@odata.count']"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Employee Territory",
  "layout": "Main"
}